{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ],
      "route": "signup_user"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "doc",
      "databaseName": "YC-Coach-DB",
      "collectionName": "Users",
      "createIfNotExists": "true",
      "connectionStringSetting": "CosmosDbConnectionString"
    }
  ]
}
