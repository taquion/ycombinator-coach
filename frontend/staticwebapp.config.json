{
  "platform": {
    "apiRuntime": "python:3.9"
  },
  "routes": [
    {
      "route": "/api/*",
      "methods": ["GET", "POST", "OPTIONS"],
      "allowedRoles": ["anonymous"]
    }
  ],
  "responseOverrides": {
    "400": {
      "rewrite": "/index.html"
    },
    "401": {
      "statusCode": 302,
      "redirect": "/.auth/login/github"
    }
  }
}
